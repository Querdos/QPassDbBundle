  "description": "Symfony bundle for storing passwords in separate database (encrypted)",
  "authors": [
    {
      "name": "Hamza ESSAYEGH",
      "email": "hamza.essayegh@protonmail.com"
    }
  ],
  "type": "symfony-bundle",
  "keywords": [
    "security encryption database passwords doctrine symfony"
  ],
  "license": [
    "MIT"
  ],
  "require": {
    "php":                                  ">=5.5.9",
    "symfony/symfony":                      "^3.2",
    "phpunit/phpunit":                      "^5.7",

    "doctrine/orm":                         "^2.5",
    "doctrine/doctrine-bundle":             "^1.6",
