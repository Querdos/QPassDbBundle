    "doctrine/doctrine-migrations-bundle":  "^1.0",
    "doctrine/dbal":                        "^2.5",
    "phpunit/php-code-coverage": "^4.0"
  },
  "require-dev": {
    "symfony/phpunit-bridge":               "^3.0",
    "fzaninotto/faker": "^1.6"
  },
  "autoload": {
    "psr-0": {
      "Querdos\\QPassDbBundle": ""
    }
  }
